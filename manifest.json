{
    "manifest_version": 3,
    "name":"Color Catcher",
    "description": "A color picker extension",
    "version": "1.0",
    "action":{
        "default_popup":"popup.html"
    },
    "icons":{
        "16":"/images/color_picker_16.png",
        "24":"/images/color_picker_24.png",
        "32":"/images/color_picker_32.png",
        "64":"/images/color_picker_64.png"
    },
    "permissions":[
        "activeTab",
        "scripting",
        "tabs",
        "storage"
    ]
    
    

}